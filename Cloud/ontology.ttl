iotpp:hasEvent rdfs:domain iotpp:Topic ;
    rdfs:range iotpp:Event ;
    rdfs:subClassOf owl:ObjectProperty .

iotpp:hasPackage rdfs:domain iotpp:Event ;
    rdfs:range iotpp:Package ;
    rdfs:subClassOf owl:ObjectProperty .

iotpp:hasSetPoint rdfs:domain iotpp:Package ;
    rdfs:range iotpp:SetPoint ;
    rdfs:subClassOf owl:ObjectProperty .

iotpp:hasValue rdfs:domain iotpp:Event ;
    rdfs:range iotpp:EventValue ;
    rdfs:subClassOf owl:ObjectProperty .

iotpp:packageId rdfs:domain iotpp:Package ;
    rdfs:range xsd:ID ;
    rdfs:subClassOf owl:DatatypeProperty .

iotpp:timestamp rdfs:domain iotpp:Event ;
    rdfs:range xsd:dateTime ;
    rdfs:subClassOf owl:DatatypeProperty .

iotpp:topicName rdfs:domain iotpp:Topic ;
    rdfs:range xsd:string ;
    rdfs:subClassOf owl:DatatypeProperty .

<https://github.com/Xitric/IoT-PerceptiveParcel#/event/1> a iotpp:Event,
        iotpp:EventValue ;
    iotpp:hasPackage <https://github.com/Xitric/IoT-PerceptiveParcel#/package/1> ;
    iotpp:hasValue <https://github.com/Xitric/IoT-PerceptiveParcel#/eventvalue/1> ;
    iotpp:timestamp "2001-10-26T21:32:52"^^xsd:dateTime .

<https://github.com/Xitric/IoT-PerceptiveParcel#/setpoint/humititySetPoint/1> a iotpp:HumititySetPoint .

<https://github.com/Xitric/IoT-PerceptiveParcel#/setpoint/motionSetPoint/1> a iotpp:MotionSetPoint .

<https://github.com/Xitric/IoT-PerceptiveParcel#/setpoint/temperaturSetPoint/1> a iotpp:TemperaturSetPoint .

iotpp:HumititySetPoint rdfs:subClassOf iotpp:SetPoint .

iotpp:HumitityViolation rdfs:subClassOf iotpp:Event .

iotpp:MotionSetPoint rdfs:subClassOf iotpp:SetPoint .

iotpp:MotionViolation rdfs:subClassOf iotpp:Event .

iotpp:TemperaturSetPoint rdfs:subClassOf iotpp:SetPoint .

iotpp:TemperatureViolation rdfs:subClassOf iotpp:Event .

iotpp:EventValue a owl:Class .

iotpp:Topic a owl:Class .

